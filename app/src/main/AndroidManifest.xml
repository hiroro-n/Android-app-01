<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="tokyo.hiroron.KanKanKanji">

    <application
            android:name="tokyo.hiroron.KanKanKanji.KankankanjiApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="tokyo.hiroron.KanKanKanji.IncorrectActivity">
        </activity>
        <activity
                android:name="tokyo.hiroron.KanKanKanji.SubActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name="tokyo.hiroron.KanKanKanji.MainActivity"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>